<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.objectsNotPrimitives.lab.controller.MainController">
    <TableView fx:id="mainTable" prefHeight="242.0" prefWidth="596.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="№" />
            <TableColumn fx:id="radColumn" prefWidth="75.0" text="Радиусы" />
            <TableColumn fx:id="dColumn" prefWidth="122.0" text="Осевые расстояния" />
            <TableColumn fx:id="nColumn" prefWidth="175.0" text="Показатели преломления" />
        </columns>
    </TableView>
    <Button fx:id="mainButton" onAction="#buttonClicked" prefHeight="30.0" prefWidth="251.0" text="Показать параксиальные характеристики" />
</VBox>
